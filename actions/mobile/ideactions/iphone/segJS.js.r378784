function segUILib() {
    var segSelectedIndex = frmHome.segFirst.selectedIndex;
    kony.print(">>>>>>>>>>>>>>>>>>>>>>>>>>>>" + segSelectedIndex);
    switch (segSelectedIndex) {
        case 0:
            frmUI.show();
            break;
        case 1:
            if (kony.os.deviceInfo().name == "Blackberry" || kony.os.deviceInfo().name == "generic" || kony.os.deviceInfo().name == "thinclient") {
                alert("Available only in iPhone and WindowsPhone");
                //kony.ui.Alert("Available only in iPhone and WindowsPhone",null,"info",null,"Info");
            } else if (kony.os.deviceInfo().name == "WindowsPhone") frmAniWin.show();
            else frmAni.show();
            break;
        case 2:
            frmData.show();
            break;
        case 3:
            frmGroup.show();
            break;
    }
}

